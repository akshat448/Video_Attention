                        "transcript": [
                            {
                                "_id": "5e1b143658934733259b544e",
                                "sentence": " I think it's really important for me to be vocal about this",
                                "start_time": 1.22,
                                "end_time": 4.38
                            },
                            {
                                "_id": "5e1b143658934733259b544d",
                                "sentence": " because I do have a platform and I represent a lot of different",
                                "start_time": 4.85,
                                "end_time": 8.93
                            },
                            {
                                "_id": "5e1b143658934733259b544c",
                                "sentence": " types of members of the deaf community.",
                                "start_time": 8.96,
                                "end_time": 11.56
                            }
                        ]
                    },
                    {
                        "_id": "5c57df360071d10d857496f7",
                        "video": "5c57d533f0d93b533389a2f7",
                        "audio_description": "5c57d533f0d93b533389a2f6",
