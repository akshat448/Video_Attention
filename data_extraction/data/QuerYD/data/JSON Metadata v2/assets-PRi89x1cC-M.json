                        "created_at": 20190204064406,
                        "__v": 0,
                        "transcript": [
                            {
                                "_id": "5e1b143558934733259b53bc",
                                "sentence": " Open captions are not for the 466 million deaf people alone.",
                                "start_time": 1.67,
                                "end_time": 6.06
                            },
                            {
                                "_id": "5e1b143558934733259b53bb",
                                "sentence": " They're beneficial for people who have AD h d people who are autistic,",
                                "start_time": 7.57,
                                "end_time": 12.03
                            },
                            {
                                "_id": "5e1b143558934733259b53ba",
                                "sentence": " people with learning disabilities, and eight from people who want to learn a new language.",
                                "start_time": 12.21,
                                "end_time": 16.88
                            },
                            {
                                "_id": "5e1b143558934733259b53b9",
                                "sentence": " Open captions are very helpful in that way",
                                "start_time": 18.23,
                                "end_time": 20.74
                            },
                            {
                                "_id": "5e1b143558934733259b53b8",
                                "sentence": " and the future.",
                                "start_time": 20.74,
                                "end_time": 21.99
                            },
                            {
                                "_id": "5e1b143558934733259b53b7",
                                "sentence": " My hope is that we have an open caption at a specific day in time",
                                "start_time": 21.99,
                                "end_time": 26.66
                            },
                            {
                                "_id": "5e1b143558934733259b53b6",
                                "sentence": " and happening at more frequent basis, so that we can bring it back to the older days where movies were more enjoyable.",
                                "start_time": 26.8,
                                "end_time": 33.53
                            },
                            {
                                "_id": "5e1b143558934733259b53b5",
                                "sentence": " We just wanna go in and sit down and watch a movie.",
                                "start_time": 33.89,
                                "end_time": 37.02
                            }
