                        "created_at": 20190204064303,
                        "__v": 0,
                        "transcript": [
                            {
                                "_id": "5e1b143558934733259b539a",
                                "sentence": " Movies are a large part of American culture.",
                                "start_time": 4.46,
                                "end_time": 6.98
                            },
                            {
                                "_id": "5e1b143558934733259b5399",
                                "sentence": " As soon as the movie is released,",
                                "start_time": 7.23,
                                "end_time": 9.29
                            },
                            {
                                "_id": "5e1b143558934733259b5398",
                                "sentence": " everybody's wanting to see what the latest releases.",
                                "start_time": 9.3,
                                "end_time": 12.4
                            },
                            {
                                "_id": "5e1b143558934733259b5397",
                                "sentence": " Unfortunately, we are part of the culture,",
                                "start_time": 12.42,
                                "end_time": 15.14
                            },
                            {
                                "_id": "5e1b143558934733259b5396",
                                "sentence": " but we're not when there's no open captioning,",
                                "start_time": 15.14,
                                "end_time": 17.6
                            },
                            {
                                "_id": "5e1b143558934733259b5395",
                                "sentence": " yeah.",
                                "start_time": 17.71,
                                "end_time": 18.48
                            },
                            {
                                "_id": "5e1b143558934733259b5394",
                                "sentence": " And the environment is not accessible to us.",
                                "start_time": 18.95,
                                "end_time": 21.8
                            },
                            {
                                "_id": "5e1b143558934733259b5393",
                                "sentence": " That means",
                                "start_time": 21.8,
                                "end_time": 22.77
                            },
                            {
                                "_id": "5e1b143558934733259b5392",
                                "sentence": " that were segregated from pop culture and popular conversation",
                                "start_time": 22.77,
                                "end_time": 26.33
                            },
                            {
                                "_id": "5e1b143558934733259b5391",
                                "sentence": " or voiceless and were left out.",
                                "start_time": 27.29,
                                "end_time": 29.35
                            }
                        ]
                    },
                    {
                        "_id": "5c57df07f0d93b533389a309",
                        "video": "5c57d533f0d93b533389a2f7",
